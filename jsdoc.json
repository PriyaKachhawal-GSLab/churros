{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "../js",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "docdash": {
    "static": true,
    "sort": true
  },
  "opts": {
    "destination": "docs/",
    "encoding": "utf8",
    "readme": "./README.md",
    "recurse": true,
    "verbose": false,
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
