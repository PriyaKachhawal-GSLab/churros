{
    "active": true,
    "configuration": [
        {
            "description": "Optional email where formula errors and notification steps will be sent (If more than one email is needed, separate each email with a comma)",
            "key": "notification.email",
            "name": "Notification Email",
            "required": false,
            "type": "value"
        },
        {
            "key": "http.request.url",
            "name": "http.request.url",
            "required": true,
            "type": "value"
        }
    ],
    "name": "http-request-successful",
    "singleThreaded": false,
    "steps": [
        {
            "name": "did-it-work",
            "onFailure": [],
            "onSuccess": [],
            "properties": {
                "body": "if (steps[\"get-sfdc-element\"].response.code === 200) {\n  done(true);\n} else {\n  done(false);\n}"
            },
            "type": "filter"
        },
        {
            "name": "get-sfdc-element",
            "onFailure": [],
            "onSuccess": [
                "did-it-work"
            ],
            "properties": {
                "method": "GET",
                "url": "config.http.request.url"
            },
            "type": "httpRequest"
        }
    ],
    "triggers": [
        {
            "async": true,
            "name": "trigger",
            "onFailure": [],
            "onSuccess": [
                "get-sfdc-element"
            ],
            "properties": {},
            "type": "manual"
        }
    ]
}
